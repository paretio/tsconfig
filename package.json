{
  "name": "@paretio/tsconfig",
  "version": "0.4.0",
  "private": false,
  "license": "MIT",
  "main": "src/index.json",
  "files": [
    "src/index.json",
    "src/*.json",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": "./src/index.json",
    "./index": "./src/index.json",
    "./base": "./src/base.json",
    "./module": "./src/module.json",
    "./non-module": "./src/non-module.json",
    "./package.json": "./package.json"
  },
  "scripts": {
    "execute-tests": "utils/execute-tests.mjs",
    "preversion": "npm run execute-tests",
    "version": "git add package.json",
    "postversion": "git push && git push --tags",
    "release:beta": "npm version prerelease --preid=beta",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/paretio/tsconfig"
  },
  "devDependencies": {
    "ts-node": "^11.0.0-beta.1"
  }
}
