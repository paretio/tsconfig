  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run openapi-generator-cli",
      "runtimeExecutable": "openapi-generator-cli",
      "cwd": "${workspaceFolder}",
      "args": []
    },
    {
      "name": "validate",
      "request": "launch",
      "type": "node-terminal",
      "command": "validate-tsconfig.mjs",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    }
  ]
}
